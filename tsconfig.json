{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "allowJs": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "noEmit": true,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "useUnknownInCatchVariables": false
  },
  "include": ["src"]
}
